{
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
          "@typescript-eslint/no-var-requires": 0
    },
    "settings": {
        "react": {
          "version": "detect"
        }
      }
  }